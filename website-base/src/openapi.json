{
    "openapi": "3.0.1",
    "info": {
      "title": "Serverless App Sync",
      "description": "Sync Serverless app code from CLI to cloud",
      "contact": {
        "name": "雷卷",
        "email": "jacky.chenlb@alibaba-inc.com"
      },
      "version": "v0.1.0"
    },
    "externalDocs": {
      "description": "SERVERLESS 开发者平台",
      "url": "http://serverless.cn/"
    },
    "servers": [
      {
        "url": "http://localhost:7001",
        "description": "Generated server url"
      }
    ],
    "tags": [
      {
        "name": "AppController",
        "description": "App REST APIs"
      },
      {
        "name": "ObjectController",
        "description": "File REST APIs"
      },
      {
        "name": "ProjectController",
        "description": "Project REST APIs"
      },
      {
        "name": "AuthController",
        "description": "Auth REST APIs"
      }
    ],
    "paths": {
      "/project/update/{domain}": {
        "post": {
          "tags": [
            "ProjectController"
          ],
          "summary": "Update project",
          "description": "更新项目的信息，主要为对应的应用信息。 注意： 不能更改项目的ID,域名这些信息。",
          "operationId": "update",
          "parameters": [
            {
              "name": "domain",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ServerlessProject"
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/OperationResult"
                  }
                }
              }
            }
          }
        }
      },
      "/project/create/{domain}": {
        "post": {
          "tags": [
            "ProjectController"
          ],
          "summary": "Create new project",
          "description": "根据域名创建新的项目，如果域名已经存在，则返回错误。创建成功后返回项目对应的UUID。 域名为Serverless平台的域名，而非自定义域名。",
          "operationId": "create",
          "parameters": [
            {
              "name": "domain",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/OperationResult"
                  }
                }
              }
            }
          }
        }
      },
      "/object/{domain}/{app}/**": {
        "post": {
          "tags": [
            "ObjectController"
          ],
          "summary": "Save app's file",
          "description": "保存应用的文件，如果成功则etag，对应的算法为MD5Hex16",
          "operationId": "save",
          "parameters": [
            {
              "name": "domain",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "app",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "body": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                }
              }
            },
            "required": true
          },
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/OperationResult"
                  }
                }
              }
            }
          }
        },
        "delete": {
          "tags": [
            "ObjectController"
          ],
          "summary": "Delete app's file",
          "description": "删除应用的文件",
          "operationId": "delete",
          "parameters": [
            {
              "name": "domain",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "app",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/OperationResult"
                  }
                }
              }
            }
          }
        }
      },
      "/project/index": {
        "get": {
          "tags": [
            "ProjectController"
          ],
          "summary": "List projects",
          "description": "列出用户所有的项目",
          "operationId": "index",
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/ServerlessProject"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "/objects/{domain}/{app}/**": {
        "get": {
          "tags": [
            "AppController"
          ],
          "summary": "List app's files",
          "description": "列出某一域名下应用对应的所有文件",
          "operationId": "objects",
          "parameters": [
            {
              "name": "domain",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "app",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            },
            {
              "name": "marker",
              "in": "query",
              "required": false,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/AppObjectListing"
                  }
                }
              }
            }
          }
        }
      },
      "/auth": {
        "get": {
          "tags": [
            "AuthController"
          ],
          "summary": "Auth JWT",
          "description": "验证JWT签名是否有效，200表示验证通过，401表示失败",
          "operationId": "auth",
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "ServerlessApp": {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "paths": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "redirects": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "object"
                }
              }
            },
            "indexFile": {
              "type": "string"
            },
            "refers": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "page404Url": {
              "type": "string"
            },
            "proxyUrl": {
              "type": "string"
            }
          }
        },
        "ServerlessProject": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "ownerId": {
              "type": "integer",
              "format": "int64"
            },
            "domain": {
              "type": "string"
            },
            "domain2": {
              "type": "string"
            },
            "favicon": {
              "type": "boolean"
            },
            "defaultApp": {
              "type": "string"
            },
            "status": {
              "type": "integer",
              "format": "int64"
            },
            "appNames": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "apps": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/ServerlessApp"
              }
            }
          }
        },
        "OperationResult": {
          "type": "object",
          "properties": {
            "success": {
              "type": "boolean"
            },
            "msg": {
              "type": "string"
            },
            "data": {
              "type": "object",
              "additionalProperties": {
                "type": "object"
              }
            }
          }
        },
        "AppObjectListing": {
          "type": "object",
          "properties": {
            "domain": {
              "type": "string"
            },
            "app": {
              "type": "string"
            },
            "marker": {
              "type": "string"
            },
            "nextMarker": {
              "type": "string"
            },
            "objectMetadataList": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/AppObjectMetadata"
              }
            }
          }
        },
        "AppObjectMetadata": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "size": {
              "type": "integer",
              "format": "int64"
            },
            "etag": {
              "type": "string"
            }
          }
        }
      }
    }
  }